<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/back"
    tools:context=".CreatedActivity">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="76dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:gravity="center"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/mission_name"
        app:layout_constraintTop_toBottomOf="@+id/mission_name" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="76dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:text=""
        android:gravity="center"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/name"
        app:layout_constraintTop_toBottomOf="@+id/mission_name" />

    <TextView
        android:id="@+id/mission_name"
        android:layout_width="298dp"
        android:layout_height="50dp"
        android:layout_marginTop="180dp"
        android:layout_weight="2"
        android:background="@null"
        android:gravity="center_horizontal"
        android:padding="10dp"
        android:shadowDx="4"
        android:shadowDy="4"
        android:shadowRadius="2"
        android:text=""
        android:textColor="#ff000000"
        android:textSize="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">/</TextView>

    <TextView
        android:id="@+id/name"
        android:layout_width="76dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:gravity="center"
        android:text="完成人:"
        android:textColor="#111111"
        app:layout_constraintStart_toStartOf="@+id/mission_name"
        app:layout_constraintTop_toBottomOf="@+id/mission_name" />

    <TextView
        android:id="@+id/money"
        android:layout_width="76dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:gravity="center"
        android:text="报酬:"
        android:textColor="#111111"
        app:layout_constraintEnd_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/mission_name" />

    <EditText
        android:id="@+id/xiangqing"
        android:layout_width="296dp"
        android:layout_height="214dp"
        android:layout_marginTop="32dp"
        android:background="@null"
        android:gravity="left"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/fabutime"
        android:layout_width="65dp"
        android:layout_height="22dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="发布时间:"
        android:textColor="#111111"
        app:layout_constraintStart_toStartOf="@+id/xiangqing"
        app:layout_constraintTop_toBottomOf="@+id/xiangqing" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="98dp"
        android:layout_height="47dp"
        android:layout_marginTop="16dp"
        android:background="@null"

        android:text=""
        android:textColor="#000000"
        android:textSize="13dp"
        app:layout_constraintStart_toEndOf="@+id/fabutime"
        app:layout_constraintTop_toBottomOf="@+id/xiangqing" />

    <TextView
        android:id="@+id/jiezhitime"
        android:layout_width="65dp"
        android:layout_height="22dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="完成时间:"
        android:textColor="#111111"
        app:layout_constraintEnd_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/xiangqing" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="79dp"
        android:layout_height="16dp"
        android:layout_marginTop="16dp"
        android:background="@null"
        android:gravity="center"
        android:text=""
        android:textColor="#000000"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="@+id/xiangqing"
        app:layout_constraintTop_toBottomOf="@+id/xiangqing" />

    <Button
        android:id="@+id/back"
        android:layout_width="45dp"
        android:layout_height="44dp"
        android:background="@mipmap/arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>